{
  "name": "elenventy.local",
  "version": "1.0.0",
  "description": "Template for using Eleventy",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --no-source-map --watch src/assets/sass/main-dev.sass:public/assets/css/main.css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass --no-source-map src/assets/sass/main-prod.sass:public/assets/css/main.css",
    "build:eleventy": "eleventy",
    "postbuild": "postcss public/assets/css/main.css -o public/assets/css/main.css",
    "start": "npm-run-all --parallel watch:*",
    "build": "npm-run-all build:sass build:eleventy",
    "clean": "rimraf ./public"
  },
  "keywords": [],
  "author": "BulgaWeb",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-bundle": "^1.0.4",
    "autoprefixer": "^10.4.14",
    "dayjs": "^1.11.7",
    "markdown-it": "^13.0.1",
    "markdown-it-bracketed-spans": "^1.0.1",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "open-props": "^1.5.8",
    "postcss": "^8.4.23",
    "postcss-jit-props": "^1.0.13",
    "postcss-minify": "^1.1.0",
    "rimraf": "^5.0.0",
    "sass": "^1.62.1"
  }
}
